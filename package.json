{
  "name": "@quinck/fastify-openapi-typescript-generator",
  "version": "1.0.0",
  "description": "Contains utilities to generate fastify types from openapi definition for the fastify framework.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "scripts": {
    "postinstall": "patch-package",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "build": "tsc",
    "test": "mocha",
    "test:junitReporter": "mocha --reporter mocha-junit-reporter",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/Quinck/fastify-openapi-typescript-generator.git"
  },
  "keywords": [
    "Fastify",
    "Openapi",
    "Generator",
    "Glue"
  ],
  "author": {
    "name": "Stefano Righini",
    "email": "stefano.righini@quinck.io"
  },
  "license": "ISC",
  "homepage": "https://bitbucket.org/Quinck/fastify-openapi-typescript-generator#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.4",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "chai": "^4.3.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "openapi-types": "^12.1.0",
    "patch-package": "^7.0.0",
    "prettier": "^2.7.1",
    "typescript": "^5.1.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "commander": "^10.0.1",
    "fastify-openapi-glue": "^4.1.4",
    "openapi-typescript": "^6.2.8",
    "yaml": "^2.2.2"
  },
  "bin": {
    "fastify-openapi-typescript": "./lib/bin/fastify-openapi-typescript.cli.js",
    "fastify-openapi-route-configs": "./lib/bin/fastify-openapi-route-configs.cli.js"
  },
  "contributors": [
    {
      "name": "Quinck s.r.l.",
      "email": "info@quinck.io"
    },
    {
      "name": "Paolo Galeotti",
      "email": "paolo.galeotti@quinck.io"
    }
  ]
}
